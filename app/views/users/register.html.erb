<div class="register_l">
	<% form_for @user, :url=>"/users/create" do |f| %>
		<ul>
			<li>
				<span class="reg_field">电子邮箱</span><%= f.text_field :email %>
			</li>
			<li>
				<span class="reg_field">昵   称</span><%= f.text_field :nick_name %>
			</li>
			<li>
				<span class="reg_field">登录密码</span><%= f.password_field :password %>
			</li>
			<li>
				<span class="reg_field">确认密码</span><%= f.password_field :password %>
			</li>
			<li>
				<%= f.submit "注册" %>
			</li>
		</ul>
	<% end %>
</div>
